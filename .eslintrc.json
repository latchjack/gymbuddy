{
    "parser": "",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": { "react/prop-types": "off", "react/react-in-jsx-scope": "off" },
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    }
}
